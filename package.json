{
	"name": "waitlist",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:api": "vercel dev",
		"dev:full": "vercel dev",
		"build": "tsc -b && vite build",
		"build:staging": "NODE_ENV=production VITE_APP_ENV=staging tsc -b && vite build",
		"build:production": "NODE_ENV=production VITE_APP_ENV=production tsc -b && vite build",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"type-check": "tsc --noEmit",
		"preview": "vite preview",
		"audit": "pnpm audit --audit-level moderate",
		"audit:fix": "pnpm audit fix --audit-level moderate",
		"outdated": "pnpm outdated"
	},
	"dependencies": {
		"@radix-ui/react-slot": "^1.2.3",
		"@tailwindcss/vite": "^4.1.14",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.546.0",
		"next-themes": "^0.4.6",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.14"
	},
	"devDependencies": {
		"@eslint/js": "^9.36.0",
		"@types/node": "^24.6.0",
		"@types/react": "^19.1.16",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.4",
		"concurrently": "^9.2.1",
		"eslint": "^9.36.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.22",
		"globals": "^16.4.0",
		"loops": "^6.0.1",
		"tw-animate-css": "^1.4.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.45.0",
		"vite": "npm:rolldown-vite@7.1.14"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.1.14"
		},
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide"
		]
	}
}
